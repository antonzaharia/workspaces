<%= link_to 'New Workspace', new_workspace_path, class: "btn btn-primary my-3" %>

<% current_user.my_workspaces.order(created_at: :desc).each do |workspace| %>
  <div class="card mb-3">
    <div class="card-body d-flex justify-content-between">
      <h5 class="card-title"><%= workspace.name %></h5>
      <%= link_to 'Visit', workspace.url(request), target: "_blank", class: 'btn btn-outline-primary' %>
    </div>
    <div class="card-footer text-muted">
      Admin
    </div>
  </div>
<% end %>

