<%= turbo_frame_tag 'new_workspace_users' do %>
  <div class="mx-auto md:w-2/3 w-full">
    <h1 class="font-bold text-4xl">New workspace user</h1>

    <%= render "form", workspace_user: @workspace_user %>

    <%= link_to 'Cancel', workspace_home_path, class: "mt-3 btn btn-ghost" %>
  </div>
  <%= @workspace.slug %>
<% end %>